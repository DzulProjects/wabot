{
  "name": "wabot",
  "version": "1.0.0",
  "description": "Modern AI-powered WABOT with WhatsApp integration and futuristic UI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development node server.js",
    "prod": "NODE_ENV=production node server.js",
    "docker:build": "docker build -t wabot .",
    "docker:run": "docker run -p 3000:3000 --env-file .env wabot",
    "docker:compose": "docker-compose up --build",
    "health": "node healthcheck.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "ai",
    "chatbot",
    "whatsapp",
    "gemini",
    "n8n",
    "wabot"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0"
  }
}
